{
  "name": "express-app",
  "version": "1.0.0",
  "description": "Port of FastAPI app to Express.js",
  "main": "server.js",
  "scripts": {
    "start": "concurrently npm:start-api npm:start-mcp",
    "start-api": "node server.js",
    "start-mcp": "node mcp-server.js",
    "dev": "nodemon server.js",
    "test": "dotenv -e .env.test -- jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "create-db": "npx sequelize-cli db:create",
    "reset-db": "npx sequelize-cli db:drop && npx sequelize-cli db:create"
  },
