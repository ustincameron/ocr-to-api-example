{
  "name": "express-app",
  "version": "1.0.0",
  "description": "Port of FastAPI app to Express.js",
  "main": "server.js",
  "scripts": {
    "start": "concurrently npm:start-api npm:start-mcp",
    "start-api": "node server.js",
    "start-mcp": "node mcp-server.js",
    "dev": "nodemon server.js",
    "test": "dotenv -e .env.test -- jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:seed": "node express-app/db/seed.js",
    "create-db": "npx sequelize-cli db:create",
    "reset-db": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npm run db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.848.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^0.3.66",
    "@langchain/google-genai": "^0.2.15",
    "@langchain/ollama": "^0.2.3",
    "@langchain/openai": "^0.6.2",
    "@modelcontextprotocol/sdk": "^1.16.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "langchain": "^0.3.30",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "node-poppler": "^8.0.3",
    "node-tesseract-ocr": "^2.2.1",
    "openai": "^5.10.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@swc/core": "^1.13.2",
    "@swc/jest": "^0.2.39",
    "concurrently": "^9.2.0",
    "dotenv-cli": "^9.0.0",
    "eslint": "^9.31.0",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4"
  }
}
