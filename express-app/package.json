    "@modelcontextprotocol/sdk": "^1.7.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "form-data": "^4.0.0",
    "joi": "^17.13.3",
    "langchain": "^0.3.30",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.52.7",
    "pdf-to-img": "^4.5.0",
